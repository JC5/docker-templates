<?xml version="1.0"?>
<Container version="2">
  <Name>Firefly</Name>
  <Repository>jc5x/firefly-iii:latest</Repository>
  <Registry>https://hub.docker.com/r/jc5x/firefly-iii</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://firefly-iii.org/</Project>
  <Overview/>
  <Category/>
  <WebUI>http://[IP]:[PORT:5080]/</WebUI>
  <TemplateURL/>
  <Icon>https://camo.githubusercontent.com/729d179402476b1dd9de4ec9f5fa5910e6bf56aa/68747470733a2f2f66697265666c792d6969692e6f72672f7374617469632f696d672f6c6f676f2d736d616c6c2d6e65772e706e67</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1559311988</DateInstalled>
  <DonateText>If you like Firefly and if it helps you save lots of money, why not send me a dime for every dollar saved. This is a joke, although the Paypal form works just fine, try it!</DonateText>
  <DonateLInk>https://firefly-iii.org/about-general.html</DonateLInk>
  <Description/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5080</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/firefly/export/</HostDir>
      <ContainerDir>/var/www/firefly-iii/storage/export</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/firefly/upload/</HostDir>
      <ContainerDir>/var/www/firefly-iii/storage/upload</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>/var/www/firefly-iii</Value>
      <Name>FIREFLY_PATH</Name>
      <Key>FIREFLY_PATH</Key>
    </Variable>
    <Variable>
      <Value>7.60.0</Value>
      <Name>CURL_VERSION</Name>
      <Key>CURL_VERSION</Key>
    </Variable>
    <Variable>
      <Value>1.1.1pre6</Value>
      <Name>OPENSSL_VERSION</Name>
      <Key>OPENSSL_VERSION</Key>
    </Variable>
    <Variable>
      <Value>Random value of exactly 32 characters</Value>
      <Name>FF_APP_KEY</Name>
      <Key>FF_APP_KEY</Key>
    </Variable>
    <Variable>
      <Value>local</Value>
      <Name>FF_APP_ENV</Name>
      <Key>FF_APP_ENV</Key>
    </Variable>
    <Variable>
      <Value>DB_UP:DB_PORT - IP:3306</Value>
      <Name>FF_DB_HOST</Name>
      <Key>FF_DB_HOST</Key>
    </Variable>
    <Variable>
      <Value>fireflydb</Value>
      <Name>FF_DB_NAME</Name>
      <key>FF_DB_NAME</key>
    </Variable>
    <Variable>
      <Value>firefly</Value>
      <Name>FF_DB_USER</Name>
      <Key>FF_DB_USER</Key>
    </Variable>
    <Variable>
      <Value>firefly</Value>
      <Name>FF_DB_PASSWORD</Name>
      <Key>FF_DB_PASSWORD</Key>
    </Variable>
  </Environment>
</Container>
